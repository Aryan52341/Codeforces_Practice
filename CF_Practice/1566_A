#include<bits/stdc++.h>
using namespace std;

const char n1='\n';
const int mod=1e9+7;



int main(){
    
    ios_base::sync_with_stdio(false);
    cin.tie(0);
   
    
    int t;
    cin>>t;

    while(t--){

       int n,s;
       cin>>n>>s;
       

        int d1=n-ceil(n/2.0)+1;

        s/=d1;
        cout<<s;

        
        
       
       cout<<n1;
  
        
        
    }
   
   
}
